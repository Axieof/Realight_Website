@model IEnumerable<Realight_Website.Models.Room>

@{
    ViewData["Title"] = "Browse";
}

<div class="browse-header">
    <h1>Browse</h1>
</div>

<p>
    <a asp-action="Create">Create New</a>
</p>
<div class="browse-container">
    @foreach (var item in Model)
    {
    <div class="map-panel" href="">
        <div class="map-header">
            <div class="map-header-info">@Html.DisplayFor(modelItem => item.name)</div>
        </div>
        <div class="map-info">
            @Html.DisplayFor(modelItem => item.code)<br />
            @Html.DisplayFor(modelItem => item.mapURL)
            @Html.DisplayFor(modelItem => item.playerCount)
            @Html.DisplayFor(modelItem => item.imgURL)
